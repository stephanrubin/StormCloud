<h1>Listing wisps</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wisps.each do |wisp| %>
  <tr>
    <td><%= wisp.content %></td>
    <td><%= wisp.user_id %></td>
    <td><%= link_to 'Show', wisp %></td>
    <td><%= link_to 'Edit', edit_wisp_path(wisp) %></td>
    <td><%= link_to 'Destroy', wisp, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wisp', new_wisp_path %>
